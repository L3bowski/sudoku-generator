{
    "name": "sudoku-viewer",
    "version": "1.0.0",
    "description": "A web project to illustrate how sudoku generation works",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean:build && webpack --mode production",
        "build:ts": "tsc --noEmit -p tsconfig.json",
        "clean:build": "rm -rf dist",
        "clean:dependencies": "rm -rf node_modules/ package-lock.json",
        "start:dev": "webpack-dev-server --open --mode development"
    },
    "author": "Carles Capellas <capellas.carles@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "react": "16.13.1",
        "react-dom": "16.13.1"
    },
    "devDependencies": {
        "@types/react": "16.9.34",
        "@types/react-dom": "16.9.6",
        "awesome-typescript-loader": "5.2.1",
        "html-loader": "1.1.0",
        "html-webpack-plugin": "4.2.0",
        "typescript": "3.8.3",
        "webpack": "4.42.1",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3"
    }
}
